@model List<string>

@using (Html.BeginForm()) { 
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>

        <tr>
            <th> FileName</th>
            <th> Link</th>
        </tr>
        @for (var i = 0; i <= Model.Count - 1; i++)
        {
            <tr>
                <td>@Model[i].ToString()</td>
                <td>@Html.ActionLink("Download", "Download", new { CsvName = @Model[i].ToString() }) </td>

            </tr >
            }
    </table>
</body>
</html>
}
