

@using (Html.BeginForm())
{
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body>

        <table>
            <thead>
                <tr>
                    <th> Archivo</th>
                    <th> Tamaño</th>
                    <th> Tipo</th>
                    <th> Fecha</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var DOCS in Model)
                {
                <tr>
                    <td>@DOCS.Archivo</td>
                    <td>@DOCS.Tamaño</td>
                    <td>@DOCS.tipo</td>
                    <td>@DOCS.Fecha</td>
                    <td>@Html.ActionLink("Download", "Download", new { CsvName = @DOCS.Archivo}) </td>
                </tr>
                }
            </tbody>
        </table>
    </body>
</html>
}
